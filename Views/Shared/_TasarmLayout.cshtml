    <script>
        var countDownDate = new Date("6/18/2023").getTime();

        var x = setInterval(function () {

            // Sistem saatini alıyoruz
            var now = new Date().getTime();

            // Geri sayımla şimdi saat arasındaki zamanı ölçüyoruz
            var distance = countDownDate - now;

            // Gün, Saat, Dakika, Saniye işlemleri yapılıyor
            var days = Math.floor(distance / (1000 * 60 * 60 * 24));
            var hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
            var minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
            var seconds = Math.floor((distance % (1000 * 60)) / 1000);

            // Anlık olarak id="gerisayim" içine aktarılıyor
            document.getElementById("gun").innerHTML = days.toString().trim();
            document.getElementById("saat").innerHTML = hours.toString().trim();
            document.getElementById("dakika").innerHTML = minutes.toString().trim();
            document.getElementById("saniye").innerHTML = +seconds.toString().trim();

            // Süre dolduğunda yazacak yazı.
            if (distance < 0) {
                clearInterval(x);
                document.getElementById("gerisayim").innerHTML = "Süre doldu";
            }
        }, 1000);

    </script>

